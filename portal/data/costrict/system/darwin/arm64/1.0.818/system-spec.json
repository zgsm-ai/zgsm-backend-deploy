{
    "configuration": "1.0.1",
    "platform": "windows",
    "arch": "arm64",
    "version": "1.0.818",
    "manager": {
        "component": {
            "name": "costrict",
            "version": "^1.0.0",
            "upgrade": {
                "mode": "auto",
                "lowest": "1.0.1",
                "highest": "1.2.3"
            }
        },
        "service": {
            "name": "costrict",
            "startup": "always",
            "command": "{{.ProcessPath}}",
            "args": [
                "server",
                "--listen",
                "localhost:{{.LocalPort}}"
            ],
            "protocol": "http",
            "port": 8999,
            "metrics": "",
            "accessible": "local"
        }
    },
    "components": [
        {
            "name": "codebase-indexer",
            "version": "^1.0.0",
            "upgrade": {
                "mode": "auto",
                "lowest": "1.0.0",
                "highest": "2.0.0"
            }
        },
        {
            "name": "cotun",
            "version": "~1.0.0"
        }
    ],
    "services": [
        {
            "name": "codebase-indexer",
            "startup": "always",
            "command": "{{.ProcessPath}}",
            "args": [
                "-http",
                "localhost:{{.LocalPort}}"
            ],
            "protocol": "http",
            "port": 9001,
            "metrics": "",
            "accessible": "remote"
        }
    ]
}
